class A
{
	public void printA()
	{
		System.out.println("A");
		
	}
}
class B extends A
{
	public void printB()
	{System.out.println("B");
		
	}
}
class C extends B
{
	public void printC()
	{System.out.println("C");
		
	}
}

class D extends C
{
	public void printD()
	{System.out.println("D");
		
	}
}

public class MyHierarichalInheritance {
	public static void main(String args[])
{
	D d=new D();
	d.printA();
	d.printB();
	d.printC();
	d.printD();
	
}

}
