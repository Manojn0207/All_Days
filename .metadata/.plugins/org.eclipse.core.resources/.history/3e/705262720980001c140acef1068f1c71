package Mystatic;
class employee {
	String name;
	int salary;
	String dept;

	employee(String name, int salary, String dept) {
		this.name = name;
		this.salary = salary;
		this.dept = dept;
	}
}

public class MyConstructor {
	public static void main(String args[]) {
		employee e1 = new employee("Ajith", 25000, "Java");
		System.out.println("Name = " + e1.name + " Salary = " + e1.salary + " Dept = " + e1.dept);
	}

}
