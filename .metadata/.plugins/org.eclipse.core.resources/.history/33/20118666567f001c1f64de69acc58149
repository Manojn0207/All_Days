import java.util.Scanner;

public class MyIfElse {
	public static void main(String args[])
	{
		Scanner var=new Scanner(System.in);
		System.out.println("Enter the number ");
		int a=var.nextInt();
		if(a>0)
		{
			System.out.println("Even");
		}
		else
		{
			System.out.println("false");
			
		}
				
	}

}
